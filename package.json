{
  "name": "trigen-frontend-boilerplate",
  "version": "0.5.1",
  "description": "Boilerplate for frontend development.",
  "main": "gulpfile.js",
  "repository": "https://github.com/TrigenSoftware/trigen-frontend-boilerplate",
  "author": "dangreen",
  "license": "MIT",
  "engines": {
    "node": ">=6.4.0"
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "test": "gulp html:lint && gulp style:lint && gulp script:lint"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "v4.0.0-alpha.6"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-0": "^6.22.0",
    "babili-webpack-plugin": "^0.0.10",
    "eslint-config-trigen": "^1.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cli": "^1.2.2",
    "gulp-connect": "^5.0.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-html-linter": "^1.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-notify": "^3.0.0",
    "gulp-progressive-css": "^1.1.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-srcset": "^1.0.1",
    "gulp-stylelint": "^3.8.0",
    "gulp-util": "^3.0.8",
    "htmllint-config-trigen": "^1.0.2",
    "immutability-helper": "^2.1.1",
    "sass-modules-importer": "^1.0.1",
    "stylelint-config-trigen": "^1.3.0",
    "webpack": "^2.2.1"
  },
  "browsers": "> 1%, last 2 versions, iOS > 7, Android > 4.4, not OperaMini all",
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": true,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "iOS > 7",
              "Android > 4.4",
              "not OperaMini all"
            ]
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
